{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": false
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": false
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        
    ],
    "globals": {
        "chrome": false,
        "App_style": false,
        "ReactDOM": false,
        "Cropper": false
    },
    "rules": {
		"no-unused-vars": ["warn", { "varsIgnorePattern": "^(App_style|style__AppRoot__js|)$" }],
		"no-mixed-spaces-and-tabs": 0,
        "no-console": "warn",
        "no-irregular-whitespace": 0
    }
}